<%= erb :"partials/includes" %>

<div class="login">
  <h2 class="ui header">Log In</h2>
  <form class="ui login form" method="post">
      <div class="field">
        <input
          name="user[username]"
          placeholder="Username"
        >
      </div>
      <div class="field">
        <input
          name="user[password]"
          placeholder="Password"
          type="password"
        >
      </div>
    <button class="ui basic button try">
      Try
    </button>
    <button class="ui basic button register">
      Register
    </button>
    <button class="ui right floated secondary button">
      Login
    </button>
  </form>
</div>
